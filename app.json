{
  "expo": {
    "name": "days",
    "slug": "days",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/layout/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/layout/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.safiexpo.days",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/layout/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/layout/logo.png",
          "backgroundColor": "#1D4ED8",
          "dark": {
            "image": "./assets/images/layout/logo-dark.png",
            "backgroundColor": "#000000"
          },
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa266dcf-a9dc-46f0-bd7b-6f8be8b103ea"
      }
    }
  }
}
